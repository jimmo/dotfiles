[user]
	name = Jim Mussared
	email = jim.mussared@gmail.com

[credential]
	helper = store

[color]
	diff = auto
	status = auto
	branch = auto

[alias]
	st = status
	co = checkout
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	undo = reset --soft HEAD^

[push]
	default = simple
